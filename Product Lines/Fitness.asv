end

for  i = 1:sizeofP
    f_temp = Problem(P(i),D);
    for j = 1:sizeofP
        if j == i
            continue;
        end
        indicator = epsIndicator(Problem(P(j),D),f_temp,ObjectiveDimension);
        Fitness(i) = Fitness(i) + ;
    end
end
end

