function [ f ] = DTLZ1(x,n)
M = 3; %Here can set up the M
k = 5; %Let k = 5, as suggested. Thus n should be 8
%get g
sum = 0;
for i = M:M+k-1
    sum = sum + ((x(i)-0.5)^2-cos(20*pi*(x(i)-0.5)));
end
g = 100 * (k + sum);
%get f
f = zeros(1,M);
for i = 1:M-1
    f(i) = 0.5*prod(x(1:M-i))*(1+g);
end
f(M) = 0.
end

